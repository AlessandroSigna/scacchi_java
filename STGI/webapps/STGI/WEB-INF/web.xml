<?xml version="1.0" encoding="utf-8"?>
<web-app>
	
	<display-name>STGI Ajedrez</display-name>
	<description>Trabajo STGI</description>
	
	<servlet>
		<servlet-name>Prova</servlet-name>
		<description>Prova</description>
		<servlet-class>Prova</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Prova</servlet-name>
		<url-pattern>/prova</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>Dashboard</servlet-name>
		<description>Dashboard</description>
		<servlet-class>Dashboard</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Dashboard</servlet-name>
		<url-pattern>/</url-pattern>
	</servlet-mapping>
	
	
	<servlet>
		<servlet-name>Login</servlet-name>
		<description>Login Page</description>
		<servlet-class>Login</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Login</servlet-name>
		<url-pattern>/login</url-pattern>
	</servlet-mapping>
	
	
	<servlet>
		<servlet-name>Signup</servlet-name>
		<description>Signup Page</description>
		<servlet-class>Signup</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Signup</servlet-name>
		<url-pattern>/signup</url-pattern>
	</servlet-mapping>
	
	
	<servlet>
		<servlet-name>Profile</servlet-name>
		<description>Profile Page</description>
		<servlet-class>Profile</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Profile</servlet-name>
		<url-pattern>/profile</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>History</servlet-name>
		<description>Game Details</description>
		<servlet-class>History</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>History</servlet-name>
		<url-pattern>/history</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>Game</servlet-name>
		<description>Game</description>
		<servlet-class>Game</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Game</servlet-name>
		<url-pattern>/game</url-pattern>
	</servlet-mapping>
	
	<session-config>
		<session-timeout>30</session-timeout>
	</session-config>
</web-app>
